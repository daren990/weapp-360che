.flipview{height:340rpx}
.flipview swiper-item{
    position: relative;
    overflow: hidden;
    height:340rpx;
}
.flipview swiper-item image{
    width:100%;
}
.flipview swiper-item text{
    position: absolute;
    bottom: 0;
    left: 0;
    font-size:28rpx;
    color: #fff;
    display: block;
    width: 100%;
    margin-bottom: 24rpx;
    padding: 0 70rpx 0 24rpx;
    box-sizing: border-box;
    height: 34rpx;
    line-height: 34rpx;
    overflow: hidden;
}
.project-item{
    position: relative;
    margin-top: 20rpx;
    overflow: hidden;
    height:200rpx;
}
.project-item image{
    width:100%;
    height:100%;
}
.project-item text{
    position: absolute;
    bottom: 14rpx;
    right: 20rpx;
    color: rgba(255,255,255,0.6);
    font-size:28rpx;
}
